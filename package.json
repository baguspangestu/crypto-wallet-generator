{
  "name": "crypto-wallet-generator",
  "version": "1.0.0",
  "main": "./src/app.js",
  "bin": "./src/app.js",
  "scripts": {
    "start": "node ./src/app.js",
    "build": "npx pkg . --out-path build"
  },
  "pkg": {
    "assets": [
      ".env",
      "node_modules/figlet/fonts/Big.flf"
    ],
    "targets": [
      "node18-linux-x64",
      "node18-macos-x64",
      "node18-win-x64"
    ]
  },
  "keywords": [
    "crypto",
    "wallet",
    "generator"
  ],
  "author": "Bagus Pangestu",
  "license": "ISC",
  "description": "Crypto Wallet Generator",
  "dependencies": {
    "dotenv": "^16.4.5",
    "ethers": "^6.13.1",
    "figlet": "^1.7.0",
    "inquirer": "^10.1.0"
  },
  "devDependencies": {
    "pkg": "^5.8.1"
  }
}
